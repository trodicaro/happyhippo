    <script type="text/javascript">
      Highcharts.chart('chart1', {
        chart: {
            type: 'area'
        },
        title: {
            text: 'Monthly Percentage Food Group Consumption'
        },

        xAxis: {
            categories: [ 'April', 'May', 'June', 'July', 'Aug', 'Sep'],
            tickmarkPlacement: 'on',
            title: {
                enabled: false
            }
        },
        yAxis: {
            title: {
                text: 'Percent'
            }
        },
        tooltip: {
            pointFormat: '<span style="color:{series.color}">{series.name}</span>: <b>{point.percentage:.1f}%</b> ({point.y:,.0f} pounds)<br/>',
            split: true
        },
        plotOptions: {
            area: {
                stacking: 'percent',
                lineColor: '#ffffff',
                lineWidth: 1,
                marker: {
                    lineWidth: 1,
                    lineColor: '#ffffff'
                }
            }
        },
        series: [{
            name: 'Bread,Cereal',
            data: [5, 6, 8, 9, 14, 36]
        }, {
            name: 'Fruit,Vegetable',
            data: [10, 11, 9, 6, 2, 7]
        }, {
            name: 'Meat,Milk',
            data: [1, 2, 2, 4, 5, 7]
        }, {
            name: 'Fats',
            data: [1, 3, 5, 1, 3, 8]
        }]
      });

      Highcharts.chart('chart2', {
        chart: {
            type: 'column'
        },
        title: {
            text: 'My Weight Trend (Lbs)'
        },
        xAxis: {

            categories: ['Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep']
        },
         yAxis: {

           title: {text:'Lbs'}
        },
        credits: {
            enabled: false
        },
        series: [{
            name: 'Ideal',
            data: [150, 150, 150, 150,150, 150]
        }, {
            name: 'Difference',
            data: [-5, 0, 10, 20, 22,26]
        }, {
            name: 'Actual',
            data: [145, 150, 160, 170,172, 176]
        }]
      });

      var bloodPressureRanges = [
              ['Apr', 90, 120],
              ['May', 90, 120],
              ['June', 90, 120],
              ['July', 90, 120],
              ['August', 90, 120],
              ['Sep', 90, 120]
          ],
          averages = [
            ['Apr', 115],
              ['May', 117],
              ['June', 118],
              ['July', 118],
              ['August', 120],
              ['Sep', 125]
          ];

      Highcharts.chart('chart3', {
        title: {
            text: 'My Systolic Blood Pressure Trend'
        },

        xAxis: {
             categories: ['Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep']
        },

        yAxis: {
            title: {
                text: 'Systolic Blood Pressure'
            }
        },

        tooltip: {
            crosshairs: true,
            shared: true,
            valueSuffix: 'Â°C'
        },

        legend: {
        },

        series: [{
            name: 'Systolic Blood Pressure',
            data: averages,
            zIndex: 1,
            marker: {
                fillColor: 'white',
                lineWidth: 2,
                lineColor: Highcharts.getOptions().colors[0]
            }
        }, {
            name: 'Range',
            data: bloodPressureRanges,
            type: 'arearange',
            lineWidth: 0,
            linkedTo: ':previous',
            color: Highcharts.getOptions().colors[0],
            fillOpacity: 0.3,
            zIndex: 0,
            marker: {
                enabled: false
            }
        }]
      });

      var cholesterolRanges = [
              ['Apr', 0, 200],
              ['May', 0, 200],
              ['June', 0, 200],
              ['July', 0, 200],
              ['August', 0, 200],
              ['Sep', 0, 200]
          ],
          averages = [
            ['Apr', 190],
              ['May', 195],
              ['June', 197],
              ['July', 198],
              ['August', 201],
              ['Sep', 210]
          ];

      Highcharts.chart('chart4', {
        title: {
            text: 'My Total Cholesterol Trend'
        },
        xAxis: {
             categories: ['Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep']
        },
        yAxis: {
            title: {
                text: 'Total Cholesterol'
            }
        },
        tooltip: {
            crosshairs: true,
            shared: true,
            valueSuffix: 'mg/dL'
        },
        legend: {
        },
        series: [{
            name: 'Total Cholesterol',
            data: averages,
            zIndex: 1,
            marker: {
                fillColor: 'white',
                lineWidth: 2,
                lineColor: Highcharts.getOptions().colors[0]
            }
        }, {
            name: 'Range',
            data: cholesterolRanges,
            type: 'arearange',
            lineWidth: 0,
            linkedTo: ':previous',
            color: Highcharts.getOptions().colors[0],
            fillOpacity: 0.3,
            zIndex: 0,
            marker: {
                enabled: false
            }
        }]
      });
    </script>
